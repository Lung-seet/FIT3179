{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "title": "House Prices of Melbourne",
    "projection": {
        "type": "equirectangular"
    },
    "data": {
        "url": "data/melbourne_house_prices.csv"
    },
    "transform": [
        {
            "lookup": "Suburb",
            "from": {
                "data": {
                    "url": "data/victoria.topo.json",
                    "format": {
                        "type": "topojson",
                        "feature": "victoria.geo"
                    }
                },
                "key": "properties.vic_loca_2"
            },
            "as": "sub"
        }
    ],
    "mark": {
        "type": "geoshape"
    },
    "encoding": {
        "shape": {
            "field": "sub",
            "type": "geojson"
        },
        "color": {
            "field": "Price", "type": "quantitative"
        },
        "tooltip": [
            {
                "field": "Suburb",
                "type": "nominal"
            },
            {
                "field": "datum.Price",
                "type": "quantitative"
            }
        ]
    }
}